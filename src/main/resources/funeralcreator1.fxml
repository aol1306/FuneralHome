<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="700.0" stylesheets="@style.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.FuneralCreator1">
    <children>
      <HBox maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
                <VBox fx:id="vbox1" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Label text="Data pogrzebu" />
                        <DatePicker fx:id="datePicker" />
                        <Label text="Imię klienta" />
                        <TextField fx:id="clientName" />
                        <Label text="Nazwisko klienta" />
                        <TextField fx:id="clientSurname" />
                        <Label text="Nr telefonu klienta" />
                        <TextField fx:id="clientPhoneNumber" />
                    </children>
                </VBox>
                <VBox fx:id="vbox2" prefHeight="400.0" prefWidth="209.0">
                    <children>
                        <Label fx:id="dateSuggestionLabel" styleClass="suggestion-label" /> <!-- text="${'Termin niedostępny\nSugerowany termin: 15/07/2020'}" /> -->
                    </children>
                </VBox>
         </children>
      </HBox>
        <Button fx:id="backButton" mnemonicParsing="false" prefWidth="60.0" styleClass="flow-button" text="Wstecz" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="70.0" />
        <Button fx:id="nextButton" mnemonicParsing="false" prefWidth="60.0" styleClass="flow-button" text="Dalej" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" />
    </children>
</AnchorPane>
